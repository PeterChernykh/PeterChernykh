@model IEnumerable< ALvl_ExamProject.MVC.Models.CategoryPL>

    <h4>Category</h4>
    <hr />
 
    <div class="list-inline">
        @foreach (var item in Model)
        {
            <a class="list-group-item list-group-item-action" href="/shop/category/@item.Slug">@item.Name</a>
        }

    </div>